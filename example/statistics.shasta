mean = {(/ (sum $) (length $))}

population_deviation = {|x|
  (Math.sqrt
    (/
      (sum 
        (map x {(pow (- $ (mean x)) 2)})
      )
      (length x)
    )
  )
}
sample_deviation = {|x|
  (Math.sqrt
    (/
      (sum 
        (map x {(pow (- $ (mean x)) 2)})
      )
      (length x)
    )
  )
}


